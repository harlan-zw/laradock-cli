{
    "name": "loonpwn/laradock-cli",
    "description": "Unofficial Laradock Command Line Tool (CLI).",
    "keywords": ["framework", "laradock", "laravel", "laravel zero", "console", "cli"],
    "homepage": "https://github.com/loonpwn/laradock-cli",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/loonpwn/laradock-cli/issues",
        "source": "https://github.com/loonpwn/laradock-cli"
    },
    "authors": [
        {
            "name": "Harlan Wilton",
            "email": "harlan@harlanzw.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "laravel-zero/framework": "^5.8",
        "spatie/emoji": "^2.1",
        "symfony/yaml": "^4.2",
        "vlucas/phpdotenv": "^3.3",
        "zendframework/zend-text": "^2.7"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "files": [
            "./bootstrap/helpers.php"
        ],
        "psr-4": {
            "Laradock\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-posix": "0"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php application app:rename"
        ],
      "test": "phpunit",
      "build": "./laradock app:build"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["laradock"]
}
